<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9"
            fx:controller="SpellCheckGUI">
    <children>
        <TextArea fx:id="textArea" editable="false" prefHeight="260.0" prefWidth="600.0" wrapText="true"/>
        <Button fx:id="previousButton" layoutX="25.0" layoutY="279.0" mnemonicParsing="false" onAction="#getPrevious"
                text="previous"/>
        <Button fx:id="nextButton" layoutX="25.0" layoutY="333.0" mnemonicParsing="false" onAction="#getNext"
                text="next"/>
        <Button fx:id="replaceButton" layoutX="214.0" layoutY="279.0" mnemonicParsing="false" onAction="#replace"
                text="replace"/>
        <TextField fx:id="textField" layoutX="300.0" layoutY="279.0"/>
        <Button fx:id="exitButton" layoutX="300.0" layoutY="333.0" mnemonicParsing="false" onAction="#exit"
                text="exit"/>
        <Button fx:id="loadButton" layoutX="216.0" layoutY="333.0" mnemonicParsing="false" onAction="#loadTxt"
                text="load"/>
    </children>
</AnchorPane>
